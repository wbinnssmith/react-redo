{
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "comma-dangle": 0,
        "func-names": 0,
        "prefer-arrow-callback": 0,
        "react/jsx-no-bind": 0,
        "no-param-reassign": 0
    }
}
